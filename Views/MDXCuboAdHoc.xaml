           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="MDXCuboAdHoc Page">
    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition Height="360"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnAnalysisServices" Content="Analysis Services" Width="200" Height="25"
                Click="btnAnalysisServices_Click" Grid.Row="0"/>
        <WebBrowser x:Name="OlapContent" Width="600" Height="400" Grid.Row="1"/>
    </Grid>
</navigation:Page>
